@page "/articles"

<style>
    body {
        background-color: #0f292f;
    }
</style>

<h3>Blog</h3>

<div class="container">
    <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
            @foreach (var post in asdf)
            {
                <ArticlePreview Article=@post></ArticlePreview>
                <hr />
            }
        </div>
    </div>
</div>